{
	"name": "@rivet-gg/icons",
	"version": "25.4.1",
	"sideEffects": false,
	"files": [
		"scripts",
		"manifest.json",
		"src",
		"dist"
	],
	"scripts": {
		"postinstall": "node scripts/postinstall.js"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
