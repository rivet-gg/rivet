{
	"name": "@rivet-gg/manager-protocol",
	"private": true,
	"version": "25.1.0-rc.1",
	"files": [
		"dist",
		"src",
		"deno.json",
		"package.json"
	],
	"type": "module",
	"exports": {
		".": {
			"deno": "./src/mod.ts",
			"import": {
				"types": "./dist/mod.d.ts",
				"default": "./dist/mod.js"
			},
			"require": {
				"types": "./dist/mod.d.cts",
				"default": "./dist/mod.cjs"
			}
		},
		"./query": {
			"deno": "./src/query.ts",
			"import": {
				"types": "./dist/query.d.ts",
				"default": "./dist/query.js"
			},
			"require": {
				"types": "./dist/query.d.cts",
				"default": "./dist/query.cjs"
			}
		}
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsup src/mod.ts src/query.ts",
		"dev": "yarn build --watch",
		"check-types": "tsc --noEmit"
	},
	"dependencies": {
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@rivet-gg/actor-common": "workspace:*",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3"
	}
}
