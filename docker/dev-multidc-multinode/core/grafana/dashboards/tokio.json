				{
					"id": "groupingToMatrix",
					"options": {
						"columnField": "le",
						"emptyValue": "zero",
						"rowField": "Time",
						"valueField": "count"
					}
				},
				{
					"id": "convertFieldType",
					"options": {
						"conversions": [
							{
